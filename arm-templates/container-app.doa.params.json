{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.1",
    "parameters": {
        "userAssignedIdentityId": {
            "value": ""
        },
        "managedEnvironmentId": {
            "value": ""
        },
        "containerAppName": {
            "value": "aca-p-uks-devops-agents"
        },
        "containerAppCPU": {
            "value": "2"
        },
        "containerAppMemory": {
            "value": "4Gi"
        },
        "containerRegistryName": {
            "value": ""
        },
        "storageAccountFileShareName":{
            "value": ""
        },
        "containerName":{
            "value": ""
        },
        "containerImage":{
            "value": ""
        },
        "secret1name":{
            "value": ""
        },
        "secret1value":{
            "value": ""
        },
        "secret2name":{
            "value": ""
        },
        "secret2value": {
            "value": ""
        },
        "secret3name":{
            "value": ""
        },
        "secret3value": {
            "value": ""
        },
        "minimumReplicas": {
            "value": 0
        },
        "maximumReplicas": {
            "value": 30
        },
        "revisionSuffix":{
            "value": ""
        },
        "mountPath":{
            "value": "/adoagent/_work"
        },
        "autoscaleRules": {
            "value": [
                {
                    "name": "azure-pipelines",
                    "custom": {
                        "type": "azure-pipelines",
                        "metadata": {
                            "poolName": "Default",
                            "targetPipelinesQueueLength": "1"
                        },
                        "auth": [
                            {
                                "secretRef": "ado-pat",
                                "triggerParameter": "personalAccessToken"
                            },
                            {
                                "secretRef": "ado-org-url",
                                "triggerParameter": "organizationURL"
                            }
                        ]
                    }
                }
            ]
        }
    }
}
